{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "dev:mock": "concurrently \"npm run mock:server\" \"npm run mock:client\"",
    "mock:client": "vite --host 0.0.0.0",
    "mock:server": "json-server --watch ./mock_server/db.json --port 3001 --middlewares ./mock_server/middleware/auth.cjs --routes ./mock_server/routes.json",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "knip": "knip"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/vue-table": "^8.21.3",
    "@vee-validate/yup": "^4.15.1",
    "@vueuse/core": "^13.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.544.0",
    "ofetch": "^1.4.1",
    "pinia": "^3.0.3",
    "reka-ui": "^2.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.8",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "0.8.1",
    "concurrently": "^9.2.1",
    "json-server": "^0.17.4",
    "knip": "^5.63.1",
    "typescript": "5.9.2",
    "vite": "7.1.6",
    "vue-tsc": "^3.0.5"
  }
}